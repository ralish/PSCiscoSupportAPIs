                    <TableColumnHeader>
                        <Label>Software Release</Label>
                    </TableColumnHeader>
                    <TableColumnHeader>
                        <Label>Is Suggested?</Label>
                    </TableColumnHeader>
                    <TableColumnHeader>
                        <Label>Release Date</Label>
                    </TableColumnHeader>
                </TableHeaders>
                <TableRowEntries>
                    <TableRowEntry>
                        <TableColumnItems>
                            <TableColumnItem>
                                <PropertyName>pid</PropertyName>
                            </TableColumnItem>
                            <TableColumnItem>
                                <PropertyName>mdf_concept_name</PropertyName>
                            </TableColumnItem>
                            <TableColumnItem>
                                <PropertyName>software_type_name</PropertyName>
                            </TableColumnItem>
                            <TableColumnItem>
                                <PropertyName>release_version</PropertyName>
                            </TableColumnItem>
                            <TableColumnItem>
