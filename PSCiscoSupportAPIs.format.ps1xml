<?xml version="1.0" encoding="utf-8"?>
<Configuration>
    <ViewDefinitions>
        <!--
            Automated Software Distribution API
        -->
        <View>
            <Name>PSCiscoSupportAPIs.SoftwareChecksum</Name>
            <ViewSelectedBy>
                <TypeName>PSCiscoSupportAPIs.SoftwareChecksum</TypeName>
            </ViewSelectedBy>
            <TableControl>
                <TableHeaders>
                    <TableColumnHeader>
                        <Label>Image Name</Label>
                    </TableColumnHeader>
                    <TableColumnHeader>
                        <Label>MD5 Hash</Label>
                    </TableColumnHeader>
                    <TableColumnHeader>
                        <Label>SHA512 Hash</Label>
                    </TableColumnHeader>
                </TableHeaders>
                <TableRowEntries>
                    <TableRowEntry>
                        <TableColumnItems>
                            <TableColumnItem>
                                <PropertyName>image_name</PropertyName>
                            </TableColumnItem>
                            <TableColumnItem>
                                <PropertyName>md5_checksum</PropertyName>
                            </TableColumnItem>
                            <TableColumnItem>
                                <PropertyName>sha512_checksum</PropertyName>
                            </TableColumnItem>
                        </TableColumnItems>
                    </TableRowEntry>
                </TableRowEntries>
            </TableControl>
        </View>
        <View>
            <Name>PSCiscoSupportAPIs.SoftwareRelease</Name>
            <ViewSelectedBy>
                <TypeName>PSCiscoSupportAPIs.SoftwareRelease</TypeName>
            </ViewSelectedBy>
            <TableControl>
